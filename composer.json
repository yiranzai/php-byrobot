{
    "name": "yiranzai/byrobot",
    "type": "library",
    "description": "百应智能外呼系统API",
    "keywords": [
        "yiranzai",
        "byrobot"
    ],
    "homepage": "https://github.com/yiranzai/php-byrobot",
    "license": "MIT",
    "authors": [
        {
            "name": "yiranzai",
            "email": "wuqingdzx@gmail.com",
            "homepage": "https://blog.yiranzai.cn",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "yiranzai/tools": "~0.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Yiranzai\\Byrobot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiranzai\\Byrobot\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
